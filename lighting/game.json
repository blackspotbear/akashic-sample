{
	"width": 640,
	"height": 480,
	"fps": 30,
	"main": "./script/main.js",
	"assets": {
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"lighting": {
			"type": "text",
			"path": "text/lighting.glsl"
		},
		"dotalpha": {
			"type": "image",
			"width": 640,
			"height": 480,
			"path": "image/dotalpha.png"
		},
		"wall_brick": {
			"type": "image",
			"width": 640,
			"height": 480,
			"path": "image/wall_brick.png"
		}
	},
	"renderers": [
		"webgl"
	],
	"environment": {
		"sandbox-runtime": "2"
	},
	"globalScripts": []
}